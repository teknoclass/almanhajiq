{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "astrotomic/laravel-translatable": "^11.12",
        "chehivskiy/i18n-arabic": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "joisarjignesh/bigbluebutton": "^2.4",
        "jubaer/zoom-laravel": "^1.3",
        "kreait/laravel-firebase": "^5.3",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.13",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.8",
        "spatie/laravel-permission": "^5.10",
        "stripe/stripe-php": "^15.0",
        "symfony/http-client": "^6.3",
        "symfony/mailgun-mailer": "^6.3",
        "symfony/postmark-mailer": "^6.3",
        "twilio/sdk": "^8.3",
        "yajra/laravel-datatables-oracle": "^10.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helper/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "php -r \"file_exists('.husky/pre-commit') || copy('.husky/pre-commit.example', '.husky/pre-commit');\"",
            "php -r \"chmod('.husky/pre-commit', 0755);\""
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "php -r \"file_exists('.husky/pre-commit') || copy('.husky/pre-commit.example', '.husky/pre-commit');\"",
            "php -r \"chmod('.husky/pre-commit', 0755);\""
        ],
        "security-check": [
            "bash ./security-check.sh"
        ],
        "security-test": [
            "bash ./security-test.sh"
        ],
        "lint": [
            "./vendor/bin/php-cs-fixer fix --dry-run --diff",
            "./vendor/bin/phpstan analyse --level=5",
            "./vendor/bin/pint --test"
        ],
        "lint-fix": [
            "./vendor/bin/php-cs-fixer fix",
            "./vendor/bin/pint"
        ],
        "pre-commit": [
            "bash .husky/pre-commit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}